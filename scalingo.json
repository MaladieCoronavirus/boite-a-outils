{
  "name": "Outils Coronavirus",
  "stack": "scalingo-18",
  "repository": "https://github.com/MaladieCoronavirus/boite-a-outils",
  "website": "https://www.outilscoronavirus.fr",
  "env": {
    "CANONICAL_HOST_URL": {
      "generator": "template",
      "template": "%APP%.osc-fr1.scalingo.io"
    },
    "DISABLE_DATADOG_AGENT": {
      "value": "true"
    },
    "DD_LOGS_ENABLED": {
      "value": "false"
    },
    "DD_PROCESS_AGENT": {
      "value": "false"
    },
    "DD_API_KEY": {
      "value": ""
    }
  },
  "formation": {
    "web": {
      "amount": 1,
      "size": "S"
    }
  }
}
